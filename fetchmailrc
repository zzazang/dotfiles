# set accout settings on ~/.netrc
poll mail.greencar.co.kr
with protocol POP3
uidl
user "chcha@greencar.co.kr"
#mda "/usr/bin/procmail -f %F -d %T"
mda "/usr/bin/msmtp --account hiworks --from='%F' -- chcha.greencar@gmail.com"
options
antispam -1
keep
no rewrite
fastuidl 4
set no bouncemail
set daemon 90
set logfile logs/fetchmail.log

